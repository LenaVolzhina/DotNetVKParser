<Window x:Class="Parser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Parser"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid Margin="0,0,-8,-20">
        <Grid HorizontalAlignment="Left" Height="320" Margin="31,10,0,0" VerticalAlignment="Top" Width="494">
            <Grid.RowDefinitions>
                <RowDefinition Height="63*"/>
                <RowDefinition Height="113*"/>
                <RowDefinition Height="77*"/>
            </Grid.RowDefinitions>
            <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="20" Margin="10,10,0,0" TextWrapping="Wrap" Text="Give me id" VerticalAlignment="Top" Width="187"/>
            <Button x:Name="buttonSearch" Content="Search" HorizontalAlignment="Left" Height="20" Margin="299,10,0,0" VerticalAlignment="Top" Width="75" Click="buttonSearch_Click"/>
        </Grid>

    </Grid>
</Window>
